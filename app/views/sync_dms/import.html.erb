<table>
  <thead>
    <tr>
      <% @data.first.keys.each do |header| %>
        <th><%= header %></th>
      <% end %>
    </tr>
  </thead>
  <tbody>
    <% @data.each do |row| %>
      <tr>
        <% row.values.each do |value| %>
          <td><%= value %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>